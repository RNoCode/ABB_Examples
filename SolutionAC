    <script>
        $(document).ready(function () {
            $('a[href=#top]').click(function (e) {
                e.preventDefault();
                $('html, body').animate({
                    scrollTop: 0
                }, 'slow');
                return false;
            });


            $('a[href=#contact-us]').click(function (e) {
                e.preventDefault();
                $('html,body').animate({
                        scrollTop: $("#contact-us").offset().top - 80
                    },
                    'slow');
                return false;
            });


            $('a[href=#more]').click(function (e) {
                e.preventDefault();
                $('html,body').animate({
                        scrollTop: $("#more").offset().top - 80
                    },
                    'slow');
                return false;
            });



        });
    </script>
    <script>
        $(document).ready(function () {

            (function () {
                $('.header-menu ul > li:has(ul)').append("<i class='fa fa-chevron-down'>").addClass(
                    'has-sub-menu').children('ul').addClass('sub-menu');
                $('.header-menu ul > li>i').on("click", function (e) {
                    e.preventDefault();
                    $(this).toggleClass('active').prev("ul").toggle()
                })
            }());
        });
    </script>
