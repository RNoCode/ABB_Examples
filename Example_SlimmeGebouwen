  <script type="text/javascript">
	var labels = document.querySelectorAll("div.pd-text label, div.pd-select label, div.pd-textarea label");
	var i = labels.length;
	while (i--) {
		var label = labels.item(i);
		var text = label.textContent;
		label.parentNode.classList.contains("required") && (text += " *");
		var nextElement = label.nextElementSibling;
		if (nextElement) {
			if (nextElement.tagName == 'SELECT') {
				nextElement.options[0].text = text;
			} else {
				nextElement.setAttribute("placeholder", text);
			}
			label.parentNode.removeChild(label);
		}
	}
	var elements = document.querySelectorAll('.errors, .no-label');
	Array.prototype.forEach.call(elements, function(el, i) {
		//el.parentNode.removeChild(el);
	});

	</script>


  <script type="text/javascript">

  $("#slider1").on("input change", (e)=>{
   const sliderPos = e.target.value;
   // Update the width of the foreground image
   $('.foreground-img1').css('width', `${sliderPos}%`)
   // Update the position of the slider button
   $('.slider-button1').css('left', `calc(${sliderPos}% - 28px)`)
  });

  $("#slider2").on("input change", (e)=>{
   const sliderPos = e.target.value;
   // Update the width of the foreground image
   $('.foreground-img2').css('width', `${sliderPos}%`)
   // Update the position of the slider button
   $('.slider-button2').css('left', `calc(${sliderPos}% - 28px)`)
  });

  $("#slider3").on("input change", (e)=>{
   const sliderPos = e.target.value;
   // Update the width of the foreground image
   $('.foreground-img3').css('width', `${sliderPos}%`)
   // Update the position of the slider button
   $('.slider-button3').css('left', `calc(${sliderPos}% - 28px)`)
  });

  $("#slider4").on("input change", (e)=>{
   const sliderPos = e.target.value;
   // Update the width of the foreground image
   $('.foreground-img4').css('width', `${sliderPos}%`)
   // Update the position of the slider button
   $('.slider-button4').css('left', `calc(${sliderPos}% - 28px)`)
  });

  $("#slider5").on("input change", (e)=>{
   const sliderPos = e.target.value;
   // Update the width of the foreground image
   $('.foreground-img5').css('width', `${sliderPos}%`)
   // Update the position of the slider button
   $('.slider-button5').css('left', `calc(${sliderPos}% - 28px)`)
  });

  $("#slider6").on("input change", (e)=>{
   const sliderPos = e.target.value;
   // Update the width of the foreground image
   $('.foreground-img6').css('width', `${sliderPos}%`)
   // Update the position of the slider button
   $('.slider-button6').css('left', `calc(${sliderPos}% - 28px)`)
  });

  $("#slider7").on("input change", (e)=>{
   const sliderPos = e.target.value;
   // Update the width of the foreground image
   $('.foreground-img7').css('width', `${sliderPos}%`)
   // Update the position of the slider button
   $('.slider-button7').css('left', `calc(${sliderPos}% - 28px)`)
  });

